# Zylix x86_64 arch Makefile configuration.

KERNEL_ARCH_GCC_NAME = x86_64-elf

KERNEL_ARCH_CFLAGS   =
KERNEL_ARCH_LDFLAGS  =
KERNEL_ARCH_LIBS     =

KERNEL_ARCH_OBJS     = $(ARCH_DIR)/boot.o \
                       $(ARCH_DIR)/cpu.o \
                       $(ARCH_DIR)/cpu_info.o \
                       $(ARCH_DIR)/setup_cpu.o \
                       $(ARCH_DIR)/setup_fpu.o \
                       $(ARCH_DIR)/pic.o \
                       $(ARCH_DIR)/gdt.o \
                       $(ARCH_DIR)/flush_gdt.o \
                       $(ARCH_DIR)/flush_idt.o \
                       $(ARCH_DIR)/flush_tss.o \
                       $(ARCH_DIR)/interrupts.o \
                       $(ARCH_DIR)/interrupts_isrs.o \
                       $(ARCH_DIR)/interrupts_lidt.o \
                       $(ARCH_DIR)/io.o
